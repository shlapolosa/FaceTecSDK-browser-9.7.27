{
  "builds": [
    {
      "src": "sample-apps/sample-app-ts/package.json",
      "use": "@vercel/node",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "sample-apps/sample-app-ts/$1"
    }
  ],
  "build": {
    "env": {
      "NODE_ENV": "production"
    },
    "commands": {
      "build": "cd sample-apps/sample-app-ts && npm run build",
      "start": "npm run facetec-browser-sdk"
    }
  }
}
